   keywords = {Bog,Daniel a. Roberts,The Principles of Deep Learning Theory,hardback},
   pages = {460},
   publisher = { Cambridge University Press},
   title = {The principles of deep learning theory : an effective theory approach to understanding neural networks},
   url = {https://www.saxo.com/dk/the-principles-of-deep-learning-theory_bog_9781316519332 https://www.saxo.com/dk/the-principles-of-deep-learning-theory_bog_9781316519332?utm_source=adtraction&utm_medium=affiliate&utm_campaign=adtraction&at_gd=AAA832D53C599B1BEA054DC3E61637745CEA6207},
