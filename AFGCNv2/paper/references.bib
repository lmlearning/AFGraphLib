@article{Cherniss1972,
   abstract = {The theme of marital obedience is only apparently discarded in favor of the spiritual moral of constancy which Chaucer, following Petrarch, appends to his Clerk's Tale. The Clerk's "envoy" actually reinforces the double focus, secular and spiritual, of the tale and its moral. On one hitherto unnoticed level of irony, it suggests that by ignoring Griselda as a model wife, wives may nevertheless benefit their husbands spiritually. They can provide the tribulation by which Job-like husbands are tested and proved by God. The Wife of Bath has herself suggested that a wife can be her husband's "purgatory." The extreme possibilities of perfection and imperfection in marriage, reflected in the Clerk's Tale and Envoy, and expressed as "paradise" and "purgatory," operate on a level of metaphor and imagery throughout the Merchant's Tale, where they provide a central, unifying irony. The earlier part of the tale sets up an opposition between the sensible man's view of marriage as "purgatory" and that of January, who sees it as a "paradise" on earth. The story itself presents a concrete example of marriage as "purgatory," and the fool-husband's wilfull blindness to his real condition, to the false nature of his "paradise."},
   author = {Michael D. Cherniss},
   doi = {10.2307/25093203},
   journal = {The Chaucer Review},
   pages = {235-254},
   publisher = {Penn State University Press},
   title = {The &quot;Clerk's Tale&quot; and &quot;Envoy,&quot; the Wife of Bath's Purgatory, and the &quot;Merchant's Tale&quot;},
   volume = {6},
   url = {https://www.jstor.org/stable/25093203},
